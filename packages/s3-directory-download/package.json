{
  "name": "@kidscannon/s3-directory-download",
  "version": "0.0.1",
  "description": "download directory to s3",
  "keywords": [
    "s3"
  ],
  "homepage": "https://github.com/KidsCannon/nodejs-tools/tree/main/packages/s3-directory-download",
  "repository": {
    "type": "git",
    "url": "https://github.com/KidsCannon/nodejs-tools.git",
    "directory": "packages/s3-directory-download"
  },
  "main": "dist/cjs/lib/index.js",
  "types": "dist/cjs/lib/index.d.js",
  "module": "dist/esm/lib/index.js",
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "jest --verbose",
    "build": "tsc -b && tsc -b tsconfig.esm.json",
    "clean": "node -e 'fs.rmSync(`dist`, { recursive:true, force:true })'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.66.0",
    "@kidscannon/not-empty": "0.0.1",
    "@kidscannon/paginate": "0.0.1",
    "mkdirp": "^1.0.4",
    "p-limit": "^3.1.0"
  },
  "devDependencies": {
    "@kidscannon/ls-recursive": "0.0.1",
    "@types/mkdirp": "^1.0.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/__tests__"
    ]
  },
  "bugs": {
    "url": "https://github.com/KidsCannon/nodejs-tools/issues"
  },
  "license": "MIT"
}
